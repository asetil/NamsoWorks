@model Aware.ECommerce.Model.ProductSearchResult
@{
    ViewBag.Title = string.IsNullOrEmpty(Model.Title) ? "Ürünler" : (Model.Title.Trim()+" Ürünleri");
    ViewBag.ActivePage = 2;
    ViewBag.HasProduct = true;
}

<section class="page-section breadcrumbs">
    <div class="container">
        <div class="page-header">
            <h1>@ViewBag.Title</h1>
        </div>
    </div>
</section>

@{Html.RenderPartial("_ProductListView", Model);}